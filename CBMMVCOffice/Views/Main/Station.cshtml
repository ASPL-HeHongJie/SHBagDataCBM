
@{
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
<link rel="stylesheet" href="~/css/ViewStation/station.css" />
<script src="~/js/ViewStation/station.js" type="module"></script>
<link rel="stylesheet" href="~/css/HappyScroll/happyscroll.css">
<script src="~/js/Happyscoll/happy-scroll.min.js" type="text/javascript"></script>
<script src="~/js/Echarts/echarts.min.js" type="text/javascript"></script>
<style>
    [v-cloak] {
        display: none;
    }
</style>
<div>
    <div class="content" id="station" v-cloak>
        <div class="homes">
            <div class="home_title">
                <i>
                    <img src="~/image/title-left.png">
                </i>
                <span>{{stationName}}</span>
                <i>
                    <img src="~/image/title-right.png">
                </i>
            </div>
            <div class="happy-scroll happysroll">
                <div class="happy-scroll-container" style=" height: 88.45vh;">
                    <div class="happy-scroll-content" style="width:100%;height:100%;">
                        <ul class="scl">
                            <li class="station" v-for="item in StationLoops" :key="item.ID">
                                <!-- 左上边框 -->
                                <div class="t_line_box">
                                    <i class="t_l_line"></i>
                                    <i class="l_t_line"></i>
                                </div>
                                <!-- 右上边框 -->
                                <div class="t_line_box">
                                    <i class="t_r_line"></i>
                                    <i class="r_t_line"></i>
                                </div>
                                <!-- 右下边框 -->
                                <div class="t_line_box">
                                    <i class="r_b_line"></i>
                                    <i class="b_r_line"></i>
                                </div>
                                <!-- 左下边框 -->
                                <div class="t_line_box">
                                    <i class="l_b_line"></i>
                                    <i class="b_l_line"></i>
                                </div>
                                <div v-if="item.BrandName+item.CollectType == 'Daniel1'">
                                    <ul class="leftArea" v-if="status">
                                        <li>{{ item.Name }}</li>
                                        <li>
                                            <img src="~/image/FM.png" />
                                        </li>
                                        <li>{{ item.FlowmeterManufacturer }}超声</li>
                                        <li>{{ item.FlowmeterModel }}</li>
                                        <li>{{ item.Caliber }}</li>
                                        <li>{{ item.Customer }}</li>

                                        <li @@click="Daniel1Details(item)">
                                            <el-button type="text">详情</el-button>
                                        </li>
                                    </ul>
                                    <div class="midArea" v-if="status">
                                        <ul>
                                            <li>
                                                <span class="little-title">基本信息</span>
                                            </li>
                                            <li>
                                                <span>气体流速</span>
                                                <span>{{SetValDigits("LoopDatas",homeAbbrNames,item.AbbrName,"VOGInuse",3,2)}} <span class="unit">m/s</span></span>
                                            </li>
                                            <li>
                                                <span>温度在用值</span>
                                                <span>
                                                    {{SetValDigits("LoopDatas",homeAbbrNames,item.AbbrName,"TemperatureInuse",3,2)}} <span class="unit">℃</span>
                                                </span>
                                            </li>
                                            <li>
                                                <span>压力在用值</span>
                                                <span>
                                                    {{SetValDigits("LoopDatas",homeAbbrNames,item.AbbrName,"PressureInuse",3,2) }} <span class="unit">kPa</span>
                                                </span>
                                            </li>
                                            <li>
                                                <span>平均性能</span>
                                                <span v-if="SetValDigits('LoopDatas',homeAbbrNames,item.AbbrName,'Path4APerformance',3,0) == '-----' || SetValDigits('LoopDatas',homeAbbrNames,item.AbbrName,'Path4BPerformance',3,0) == '-----'
                                                  || SetValDigits('LoopDatas',homeAbbrNames,item.AbbrName,'Path3APerformance',3,0) == '-----' || SetValDigits('LoopDatas',homeAbbrNames,item.AbbrName,'Path3BPerformance',3,0) == '-----'
                                                  ||SetValDigits('LoopDatas',homeAbbrNames,item.AbbrName,'Path2APerformance',3,0) == '-----' || SetValDigits('LoopDatas',homeAbbrNames,item.AbbrName,'Path2BPerformance',3,0) == '-----'
                                                  || SetValDigits('LoopDatas',homeAbbrNames,item.AbbrName,'Path1APerformance',3,0) == '-----' || SetValDigits('LoopDatas',homeAbbrNames,item.AbbrName,'Path1BPerformance',3,0) == '-----'">
                                                    -----<span class="unit">%</span>
                                                </span>
                                                <span v-else>
                                                    {{numberDigits((SetValDigits("LoopDatas",homeAbbrNames,item.AbbrName,"Path4APerformance",3,0)*1+SetValDigits("LoopDatas",homeAbbrNames,item.AbbrName,"Path4BPerformance",3,0)*1+SetValDigits("LoopDatas",homeAbbrNames,item.AbbrName,"Path3APerformance",3,0)*1+SetValDigits("LoopDatas",homeAbbrNames,item.AbbrName,"Path3BPerformance",3,0)*1+SetValDigits("LoopDatas",homeAbbrNames,item.AbbrName,"Path2APerformance",3,0)*1+SetValDigits("LoopDatas",homeAbbrNames,item.AbbrName,"Path2BPerformance",3)*1+SetValDigits("LoopDatas",homeAbbrNames,item.AbbrName,"Path1APerformance",3,0)*1+SetValDigits("LoopDatas",homeAbbrNames,item.AbbrName,"Path1BPerformance",3,0)*1)/8,3)}}<span class="unit">%</span>
                                                </span>
                                            </li>
                                            <li>
                                                <span>运行状态</span>
                                                <span v-if=" SetValDigits('LoopDatas',homeAbbrNames,item.AbbrName,'LoopStatus') == 0">在用</span>
                                                <span v-else-if=" SetValDigits('LoopDatas',homeAbbrNames,item.AbbrName,'LoopStatus') == 1">备用</span>
                                                <span v-else-if=" SetValDigits('LoopDatas',homeAbbrNames,item.AbbrName,'LoopStatus') == 2 || SetValDigits('LoopDatas',homeAbbrNames,item.AbbrName,'LoopStatus') =='-----'">停用</span>
                                                <span v-else-if=" SetValDigits('LoopDatas',homeAbbrNames,item.AbbrName,'LoopStatus') == 3">通讯失败</span>
                                                <span v-else-if=" SetValDigits('LoopDatas',homeAbbrNames,item.AbbrName,'LoopStatus') == 4">送检</span>
                                            </li>
                                        </ul>
                                        <ul>
                                            <li>
                                                <span class="little-title">瞬时流量</span>
                                            </li>
                                            <li>
                                                <span>工况流量</span>
                                                <span>{{SetValDigits("LoopDatas",homeAbbrNames,item.AbbrName,"GrossFlowrate",5,2)}} <span class="unit">m3/h</span></span>
                                            </li>
                                            <li>
                                                <span>标况流量</span>
                                                <span>{{ SetValDigits("LoopDatas",homeAbbrNames,item.AbbrName,"StandardFlowrate",5,2)}}<span class="unit">NKm3/h</span></span>
                                            </li>
                                            <li>
                                                <span>质量流量</span>
                                                <span>{{SetValDigits("LoopDatas",homeAbbrNames,item.AbbrName,"MassFlowrate",5,2)}}<span class="unit">t/h</span></span>
                                            </li>
                                            <li>
                                                <span>能量流量</span>
                                                <span>{{SetValDigits("LoopDatas",homeAbbrNames,item.AbbrName,"EnergyFlowrate",5,0)}} <span class="unit">MJ/h</span></span>
                                            </li>
                                        </ul>
                                    </div>
                                    <div class="rightArea" v-if="status">
                                        <ul>
                                            <li>
                                                <span class="little-title">诊断信息</span>
                                            </li>
                                            <li>
                                                <span>流量计诊断结果</span>
                                                <span style="color:red;"
                                                      v-if="SetDiagnosticDetail('LoopDatas',item.AbbrName,'fmDiagnosticResult')=='故障' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'fmDiagnosticResult') == '诊断条件不足' ||SetDiagnosticDetail('LoopDatas',item.AbbrName,'fmDiagnosticResult') == '备用' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'fmDiagnosticResult') == '送检' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'fmDiagnosticResult') == '通讯故障'">
                                                    <span v-if="SetDiagnosticDetail('LoopDatas',item.AbbrName,'fmDiagnosticResult')=='故障' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'fmDiagnosticResult')=='诊断条件不足'" @@dblclick="diagnosisMany(item,'fmDiagnosticResult',$event)" style="cursor:pointer;">
                                                        <i class="fa fa-minus-square"></i>
                                                        {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"fmDiagnosticResult")}}
                                                    </span>
                                                    <span v-else>
                                                        {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"fmDiagnosticResult")}}
                                                    </span>
                                                </span>
                                                <span style="color:#fff;"
                                                      v-else-if="SetDiagnosticDetail('LoopDatas',item.AbbrName,'fmDiagnosticResult') == '正常' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'fmDiagnosticResult') == '-----'">
                                                    <span style="cursor:pointer;" v-if="SetDiagnosticDetail('LoopDatas',item.AbbrName,'fmDiagnosticResult') == '正常'" @@dblclick="diagnosisMany(item,'fmDiagnosticResult',$event)">
                                                        <i class="fa fa-minus-square"></i>
                                                        {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"fmDiagnosticResult")}}
                                                    </span>
                                                    <span v-else>
                                                        {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"fmDiagnosticResult")}}
                                                    </span>
                                                </span>
                                                <span style="color:orange;" v-else>
                                                    {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"fmDiagnosticResult")}}
                                                </span>
                                            </li>
                                            <li>
                                                <span>压变诊断结果</span>
                                                <span style="color:red;"
                                                      v-if="SetDiagnosticDetail('LoopDatas',item.AbbrName,'ptDiagnosticResult') == '故障' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'ptDiagnosticResult') == '诊断条件不足'||
                       SetDiagnosticDetail('LoopDatas',item.AbbrName,'ptDiagnosticResult') == '备用'|| SetDiagnosticDetail('LoopDatas',item.AbbrName,'ptDiagnosticResult') == '送检'|| SetDiagnosticDetail('LoopDatas',item.AbbrName,'ptDiagnosticResult') == '通讯故障'">
                                                    <span style="cursor:pointer;" v-if="SetDiagnosticDetail('LoopDatas',item.AbbrName,'ptDiagnosticResult') == '故障' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'ptDiagnosticResult') == '诊断条件不足'" @@dblclick="diagnosisLess(item,'ptDiagnosticResult',$event)">
                                                        <i class="fa fa-minus-square"></i>
                                                        {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"ptDiagnosticResult")}}
                                                    </span>
                                                    <span v-else>
                                                        {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"ptDiagnosticResult")}}
                                                    </span>
                                                </span>
                                                <span style="color:#fff;"
                                                      v-else-if="SetDiagnosticDetail('LoopDatas',item.AbbrName,'ptDiagnosticResult') == '正常' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'ptDiagnosticResult') == '-----'">
                                                    <span style="cursor:pointer;" v-if="SetDiagnosticDetail('LoopDatas',item.AbbrName,'ptDiagnosticResult') == '正常'" @@dblclick="diagnosisLess(item,'ptDiagnosticResult',$event)">
                                                        <i class="fa fa-minus-square"></i>
                                                        {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"ptDiagnosticResult")}}
                                                    </span>
                                                    <span v-else>
                                                        {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"ptDiagnosticResult")}}
                                                    </span>
                                                </span>
                                                <span style="color:orange;" v-else>
                                                    {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"ptDiagnosticResult")}}
                                                </span>
                                            </li>
                                            <li>
                                                <span>温变诊断结果</span>
                                                <span style="color:red;"
                                                      v-if="SetDiagnosticDetail('LoopDatas',item.AbbrName,'ttDiagnosticResult') == '故障' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'ttDiagnosticResult') == '诊断条件不足' ||
                      SetDiagnosticDetail('LoopDatas',item.AbbrName,'ttDiagnosticResult') == '备用' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'ttDiagnosticResult') == '送检'|| SetDiagnosticDetail('LoopDatas',item.AbbrName,'ttDiagnosticResult') == '通讯故障'">
                                                    <span style="cursor:pointer;" v-if="SetDiagnosticDetail('LoopDatas',item.AbbrName,'ttDiagnosticResult') == '故障' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'ttDiagnosticResult') == '诊断条件不足'" @@dblclick="diagnosisLess(item,'ttDiagnosticResult',$event)">
                                                        <i class="fa fa-minus-square"></i>
                                                        {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"ttDiagnosticResult")}}
                                                    </span>
                                                    <span v-else>
                                                        {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"ttDiagnosticResult")}}
                                                    </span>
                                                </span>
                                                <span style="color:#fff;"
                                                      v-else-if="SetDiagnosticDetail('LoopDatas',item.AbbrName,'ttDiagnosticResult') == '正常' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'ttDiagnosticResult') == '-----'">
                                                    <span style="cursor:pointer;" v-if="SetDiagnosticDetail('LoopDatas',item.AbbrName,'ttDiagnosticResult') == '正常'" @@dblclick="diagnosisLess(item,'ttDiagnosticResult',$event)">
                                                        <i class="fa fa-minus-square"></i>
                                                        {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"ttDiagnosticResult")}}
                                                    </span>
                                                    <span v-else>
                                                        {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"ttDiagnosticResult")}}
                                                    </span>
                                                </span>
                                                <span style="color:orange;" v-else>
                                                    {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"ttDiagnosticResult")}}
                                                </span>
                                            </li>
                                            <li>
                                                <span>流量计算机诊断结果</span>
                                                <span style="color:red;"
                                                      v-if="SetDiagnosticDetail('LoopDatas',item.AbbrName,'fcDiagnosticResult') == '故障' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'fcDiagnosticResult') == '诊断条件不足' ||
                       SetDiagnosticDetail('LoopDatas',item.AbbrName,'fcDiagnosticResult') == '备用' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'fcDiagnosticResult') == '送检' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'fcDiagnosticResult') == '通讯故障'">
                                                    <span style="cursor:pointer;" v-if="SetDiagnosticDetail('LoopDatas',item.AbbrName,'fcDiagnosticResult') == '故障' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'fcDiagnosticResult') == '诊断条件不足'" @@dblclick="diagnosisLess(item,'fcDiagnosticResult',$event)">
                                                        <i class="fa fa-minus-square"></i>
                                                        {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"fcDiagnosticResult")}}
                                                    </span>
                                                    <span v-else>
                                                        {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"fcDiagnosticResult")}}
                                                    </span>
                                                </span>
                                                <span style="color:#fff;"
                                                      v-else-if="SetDiagnosticDetail('LoopDatas',item.AbbrName,'fcDiagnosticResult') == '正常' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'fcDiagnosticResult') == '-----'">
                                                    <span style="cursor:pointer;" v-if="SetDiagnosticDetail('LoopDatas',item.AbbrName,'fcDiagnosticResult') == '正常'" @@dblclick="diagnosisLess(item,'fcDiagnosticResult',$event)">
                                                        <i class="fa fa-minus-square"></i>
                                                        {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"fcDiagnosticResult")}}
                                                    </span>
                                                    <span v-else>
                                                        {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"fcDiagnosticResult")}}
                                                    </span>
                                                </span>
                                                <span style="color:orange;" v-else>
                                                    {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"fcDiagnosticResult")}}
                                                </span>

                                            </li>
                                            <li>
                                                <span>声速核查诊断结果</span>
                                                <span style="color:red;"
                                                      v-if="SetDiagnosticDetail('LoopDatas',item.AbbrName,'vosDiagnosticResult') == '故障' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'vosDiagnosticResult') == '诊断条件不足' ||
                       SetDiagnosticDetail('LoopDatas',item.AbbrName,'vosDiagnosticResult') == '备用' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'vosDiagnosticResult') == '送检' ||  SetDiagnosticDetail('LoopDatas',item.AbbrName,'vosDiagnosticResult') == '通讯故障'">
                                                    <span style="cursor:pointer;" v-if="SetDiagnosticDetail('LoopDatas',item.AbbrName,'vosDiagnosticResult') == '故障' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'vosDiagnosticResult') == '诊断条件不足'" @@dblclick="diagnosisMany(item,'vosDiagnosticResult',$event)">
                                                        <i class="fa fa-minus-square"></i>
                                                        {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"vosDiagnosticResult")}}
                                                    </span>
                                                    <span v-else>
                                                        {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"vosDiagnosticResult")}}
                                                    </span>
                                                </span>
                                                <span style="color:#fff;"
                                                      v-else-if="SetDiagnosticDetail('LoopDatas',item.AbbrName,'vosDiagnosticResult') == '正常' || SetDiagnosticDetail('LoopDatas',item.AbbrName,'vosDiagnosticResult') == '-----'">
                                                    <span style="cursor:pointer;" v-if="SetDiagnosticDetail('LoopDatas',item.AbbrName,'vosDiagnosticResult') == '正常'" @@dblclick="diagnosisMany(item,'vosDiagnosticResult',$event)">
                                                        <i class="fa fa-minus-square"></i>
                                                        {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"vosDiagnosticResult")}}
                                                    </span>
                                                    <span v-else>
                                                        {{SetDiagnosticDetail("LoopDatas",item.AbbrName,"vosDiagnosticResult")}}
                                                    </span>
                                                </span>
                                                <span style="color:orange;"
                                                      v-else>{{SetDiagnosticDetail("LoopDatas",item.AbbrName,"vosDiagnosticResult")}}</span>
                                            </li>
                                        </ul>
                                        <ul>
                                            <li>
                                                <span class="little-title">报警信息</span>
                                            </li>
                                            <li>
                                                <span>流量计报警</span>
                                                <span>
                                                    <i class="fa fa-bell" style="color:#1AFA29;font-size:1.2vw;" v-if="SetAlarmCountDetail('LoopDatas',item.AbbrName,'FM') == 0"></i>
                                                    <i class="fa fa-bell" style="color:red;font-size:1.2vw;" v-else-if="SetAlarmCountDetail('LoopDatas',item.AbbrName,'FM') > 0"></i>
                                                    <i class="fa fa-bell" style="color:#fff;font-size:1.2vw;" v-else></i>
                                                </span>
                                            </li>
                                            <li>
                                                <span>流量计算机报警</span>
                                                <span>
                                                    <i class="fa fa-bell" style="color:#1AFA29;font-size:1.2vw;" v-if="SetAlarmCountDetail('LoopDatas',item.AbbrName,'FC') == 0"></i>
                                                    <i class="fa fa-bell" style="color:red;font-size:1.2vw;" v-else-if="SetAlarmCountDetail('LoopDatas',item.AbbrName,'FC') > 0"></i>
                                                    <i class="fa fa-bell" style="color:#fff;font-size:1.2vw;" v-else></i>
                                                </span>
                                            </li>
                                            <li>
                                                <span>压变报警</span>
                                                <span>
                                                    <i class="fa fa-bell" style="color:#1AFA29;font-size:1.2vw;" v-if="SetAlarmCountDetail('LoopDatas',item.AbbrName,'PT') == 0"></i>
                                                    <i class="fa fa-bell" style="color:red;font-size:1.2vw;" v-else-if="SetAlarmCountDetail('LoopDatas',item.AbbrName,'PT') > 0"></i>
                                                    <i class="fa fa-bell" style="color:#fff;font-size:1.2vw;" v-else></i>
                                                </span>
                                            </li>
                                            <li>
                                                <span>温变报警</span>
                                                <span>
                                                    <i class="fa fa-bell" style="color:#1AFA29;font-size:1.2vw;" v-if="SetAlarmCountDetail('LoopDatas',item.AbbrName,'TT') == 0"></i>
                                                    <i class="fa fa-bell" style="color:red;font-size:1.2vw;" v-else-if="SetAlarmCountDetail('LoopDatas',item.AbbrName,'TT') > 0"></i>
                                                    <i class="fa fa-bell" style="color:#fff;font-size:1.2vw;" v-else></i>
                                                </span>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                                <div v-else-if="item.BrandName+item.CollectType == 'Elster1'">

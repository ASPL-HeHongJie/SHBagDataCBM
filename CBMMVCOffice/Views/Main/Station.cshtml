                                            <li>
                                                <span>气体流速</span>
                                                <span>
                                                    {{SetValDigits("LoopDatas",homeAbbrNames,item.AbbrName,"VOGInuse",3,2) }} <span class="unit">m/s</span>
                                                </span>
                                            </li>
                                            <li>
